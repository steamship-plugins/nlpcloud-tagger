{
  "type": "plugin",
  "handle": "nlpcloud-tagger",
  "version": "1.0.1",
  "description": "Tags text using the NLP Cloud API.",
  "author": "Steamship",
  "entrypoint": "api.handler",
  "public": true,
  "configTemplate": {
    "task": {
      "type": "string"
    },
    "model": {
      "type": "string"
    }
  },
  "plugin": {
    "type": "tagger",
    "path": "/tag",
    "isTrainable": false,
    "transport": "jsonOverHttp",
    "runPath": "/run"
  }
}