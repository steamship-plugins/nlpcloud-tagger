{
  "type": "plugin",
  "name": "NLPCloud Tagger",
  "handle": "nlpcloud-tagger",
  "version": "1.0.0",
  "description": "Tags text using the NLP Cloud API.",
  "author": "Steamship",
  "entrypoint": "api.handler",
  "plugin": {
    "type": "tagger",
    "path": "/tag",
    "isTrainable": false,
    "configTemplate": {
      "api_key": {
        "type": "string",
        "default": ""
      },
      "input_type": {
        "type": "string",
        "default": "article"
      },
      "skills": {
        "type": "string",
        "default": "entities"
      }
    },
    "transport": "jsonOverHttp",
    "runPath": "/run"
  }
}